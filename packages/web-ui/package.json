{
  "name": "@x-skynet/web-ui",
  "version": "0.1.0",
  "description": "Lightweight Web UI dashboard for X-Skynet \u2014 real-time agent status, DAG runs, task queue",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@xskynet/contracts": "workspace:*",
    "@xskynet/core": "workspace:*",
    "@xskynet/plugin-shell": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json"
  ]
}
