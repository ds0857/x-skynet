{
  "name": "@xskynet/plugin-memory",
  "version": "0.1.0",
  "description": "Persistent agent memory plugin for X-Skynet \u2014 JSON file backend with set/get/delete/list/search",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "typescript": ">=5.3.0"
  },
  "dependencies": {
    "@xskynet/contracts": "workspace:*",
    "@xskynet/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
