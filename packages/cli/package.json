{
  "name": "@xskynet/cli",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "X-Skynet command-line interface",
  "type": "module",
  "bin": {
    "xskynet": "dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "xskynet",
    "cli",
    "agents"
  ],
  "author": "X-Skynet Team",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ora": "^8.0.1",
    "@clack/prompts": "^0.9.0",
    "@xskynet/contracts": "workspace:*",
    "@xskynet/core": "workspace:*",
    "@xskynet/plugin-shell": "workspace:*",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "rimraf": "^5.0.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json"
  ],
  "main": "dist/index.js"
}
